<h1>Listing articles</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Title</th>
    <th>Content</th>
    <th></th>
    
  </tr>

<% @articles.each do |article| %>
  <tr>
    <td><%= article.name %></td>
    <td><%= article.title %></td>
    <td><%= article.content %></td>
    <td><%= article.created_at %></td>

    
    <td><%= link_to 'Show', article %></td>
    
  </tr>
<% end %>
</table>

<br />
