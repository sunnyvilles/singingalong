
<html>
  <head>
    <title>TheOtherSongClinic</title>
    <script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <link rel="stylesheet" href="http://code.jquery.com/mobile/1.3.1/jquery.mobile-1.3.1.min.css">
    <script src="http://code.jquery.com/mobile/1.3.1/jquery.mobile-1.3.1.min.js"></script>
    <%= javascript_include_tag "mobile" %>
   
  </head>
  <body>
    <div class="current">
      
      <% unless flash.empty? %>
        <div class="info">
        <%- flash.each do |name, msg| -%>
          <%= content_tag :div, msg, :id => "flash_#{name}" %>
        <%- end -%>
        </div>
      <% end %>
    
     <div data-role="page" id="page">
            <div data-role="header" data-theme="a">
                <h1>theothersong.com</h1>
                <a data-role="button" href="#page1" data-rel="page">back</a>
            </div>
            <div data-role="content">
                
                    <ul data-role="listview" data-inset="true" data-theme="c">

                      <li><%= link_to("About Us", {:controller => 'static' ,:action => 'about_us'}) %></li>
                      <li><%= link_to("Our Edge", {:controller => 'static' ,:action => 'our_edge'}) %></li>
                      <li><%= link_to("Knowledge Center", {:controller => 'static' ,:action => 'knowledge_center'}) %></li>
                      <li><%= link_to("Events", {:controller => 'static' ,:action => 'events'}) %></li>
                      <li><%= link_to("General Information", {:controller => 'static' ,:action => 'general_information'}) %></li>
                      
                    </ul>
                
            </div>
           </div>
             </div>  
  </body>
</html>